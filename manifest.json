{
	"manifest_version" : 2,
	"name" : "Gatunes",
	"description" : "__MSG_appDesc__",
	"version" : "1.0.0",
	"minimum_chrome_version" : "23",
	"default_locale" : "en",
	"permissions" : [
		"storage",
		"unlimitedStorage",
		"webview",
		"fullscreen",
		"audioCapture"
	],
	"sandbox": {
		"pages": ["image.html"]
	},
	"icons" : {
		"128" : "img/128.png"
	},
	"app" : {
		"background" : {
			"scripts" : ["launch.js"],
			"transient" : true
		}
	}
}
