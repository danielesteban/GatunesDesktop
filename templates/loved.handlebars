<div class="header">
	<h1 key="loved">
		{{L 'loved'}} 
		<button class="btn offline"><i class="icon-download-alt"></i> <span></span></button>
	</h1>
	<hr>
</div>
<div class="half">
	<div class="padding">
		<table>
			{{#each songs}}
				{{> song}}
			{{/each}}
			{{#empty songs}}
				<tr><td class="empty">{{L 'emptyLoved'}}</td>
			{{/empty}}
		</table>
	</div>
</div>
